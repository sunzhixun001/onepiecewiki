.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 68px;
  padding-bottom: env(safe-area-inset-bottom);
  display: flex;
  align-items: flex-end;
  flex-direction: row;
}
.tab-bar .inner {
  height: 48px;
  /* background: white; */
  display: flex;
  flex: 1;
}
.tab-bar-border {
  background-color: rgba(0, 0, 0, 0.33);
  position: absolute;
  left: 0;
  top: 20px;
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
}
.tab-bar-bg {
  background-color: #ffffff;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 48px;
}
.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  flex-direction: column;
  padding-bottom: 10px;
  z-index: 99;
}

.tab-bar-item .image {
  width: 20px;
  /* height: 20px; */
  /* background-color: blue; */
}
.tab-bar-item.c .image {
  width: 70px;
  /* height: 40px; */
}
.tab-bar-item .tab-name {
  font-size: 10px;
  height: 12px;
  /* background-color: aqua; */
}
